# Gitea target
GITEA_URL="https://gitea.example.com"
GITEA_TOKEN=""
GITEA_OWNER_MAP="" # example: org1=org1,org2=org2

# Export directory from github-issues-prs-export.sh
EXPORT_DIR="./exports/github"

# Import behavior
IMPORT_ISSUES="true"
IMPORT_PULLS="true"
PULL_REQUEST_MODE="issue" # issue|pr
IMPORT_LABELS="false"
IMPORT_ASSIGNEES="false"
IMPORT_MILESTONES="false"
CREATE_MISSING_LABELS="false"
CREATE_MISSING_MILESTONES="false"
USER_MAP="" # example: octocat=octo,alice=a.smith
USER_MAP_FILE="" # file with lines: github_user=gitea_user
SET_CLOSED_STATE="false"
DRY_RUN="true"
